{
  "cli": {
    "version": ">= 6.3.1",                     
    "appVersionSource": "remote"               
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"                     
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"           
      }
    },
    "production": {
      "autoIncrement": true,         
      "android": {
        "buildType": "app-bundle"              
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"                    
      }
    }
  }
}
